<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.MainController">
   <children>
      <ImageView fitHeight="500.0" fitWidth="500.0">
         <image>
            <Image url="@palette-312289_640.png" />
         </image>
      </ImageView>
      <BorderPane layoutX="2.0" layoutY="2.0" prefHeight="500.0" prefWidth="500.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <Button mnemonicParsing="false" onAction="#handleOptions" text="Options" textFill="#3d4ada" BorderPane.alignment="CENTER" />
         </center>
         <bottom>
            <Button mnemonicParsing="false" onAction="#handleCloseButton" text="Exit Game" textFill="#3d4ada" BorderPane.alignment="CENTER" />
         </bottom>
         <top>
            <Button mnemonicParsing="false" onAction="#handle" text="Start Game" textFill="#3d4ada" BorderPane.alignment="CENTER" />
         </top>
      </BorderPane>
   </children>
</AnchorPane>
